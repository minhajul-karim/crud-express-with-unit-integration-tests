{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "object-curly-newline": ["error", { "multiline": true }],
    "no-undef": "off",
    "no-unused-vars": "warn"
  },
  "plugins": ["prettier"]
}
